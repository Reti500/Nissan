/*GENERAL Y CATEGORIAS*/
@font-face
	font-family NissanAGBol
	src url(fonts/NissanAGBol)

@font-face
	font-family NissanAGBolExt
	src url(fonts/NissanAGBol)

@font-face
	font-family NissanAGReg
	src url(fonts/NissanAGBol)


body
	margin 0
	padding 0
	background-color #444
	font-family NissanAGBolExt

footer 
	margin 0  
	padding 0
	background-color gray 
	font-size 0.75em
	min-height 67px

h1
	margin 1% 10%

p 
	margin 0 10%
	padding 1%

video
	margin 0 10%

button
	width 290px
	height 40px
	background-color #c02c55
	color #fff
	border none 
	font-size 1.2em

button:hover
	transition .25s
	border-style solid
	border-radius .2
	border-width 0
	border-color #fff
	box-shadow 1px 1px .4px #fff, -1px -1px .4px #333




