#faq 
	text-align center
	width 800px
#faq .qcards 
	width 80%
	border-style solid
	border-width 1px
	border-radius 10px
	padding 10px
	margin 0 auto

#faq .flip-content
	position relative
	width 80%
	height auto
	margin auto auto
	top: 50%;
	transform: translateY(-50%)

#faq input[type=text]
	padding 10px
	width 300px

#faq button
	background-color #00000000
	border-width 0
	